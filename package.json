{
  "name": "krnl-onboarding-system",
  "version": "1.0.0",
  "description": "Multi-agent employee onboarding workflow system",
  "main": "index.js",
  "scripts": {
    "dev": "npm run dev:backend & npm run dev:frontend",
    "dev:backend": "cd backend && python -m uvicorn main:app --reload --port 8000",
    "dev:frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "test": "cd backend && python -m pytest tests/",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["multi-agent", "onboarding", "workflow", "MCP", "A2A"],
  "author": "KRNL Team",
  "license": "MIT"
}